# 서버 설정
REACT_APP_API_URL=http://localhost:8080/api
REACT_APP_SOCKET_URL=http://localhost:3001

# 모의 서비스 설정
REACT_APP_USE_MOCK_SERVICES=true

# 개발 환경 설정
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# 로깅 설정
REACT_APP_ENABLE_MOCK_LOGS=true  # 모의 서비스 로그 출력 여부

# 테스트 시나리오 설정
REACT_APP_MOCK_NETWORK_DELAY=300           # 기본 네트워크 지연 시간 (ms)
REACT_APP_MOCK_POOR_NETWORK_DELAY=2000     # 불안정한 네트워크 지연 시간 (ms)
REACT_APP_MOCK_ERROR_RATE=0.1              # 에러 발생 확률 (0-1)
REACT_APP_MOCK_RESPONSE_RATE=0.7           # 자동 응답 확률 (0-1)
REACT_APP_MOCK_MATCH_SUCCESS_RATE=0.6      # 매칭 성공 확률 (0-1) 